{
    "name": "Chaos Shadow",
    "type": "dark",
    "colors": {
        "editor.background": "#0a0a0a",
        "editor.foreground": "#ffffff",
        "editorLineNumber.foreground": "#4d4d4d",
        "editorCursor.foreground": "#ff0000",
        "editor.selectionBackground": "#ff000050",
        "editor.inactiveSelectionBackground": "#ff000020",
        "editor.lineHighlightBackground": "#111111",
        "editorIndentGuide.background": "#2a2a2a",
        "activityBar.background": "#1a1a1a",
        "sideBar.background": "#101010",
        "sideBar.foreground": "#ffffff",
        "tab.activeBackground": "#1a1a1a",
        "tab.inactiveBackground": "#0a0a0a",
        "tab.activeBorder": "#ff0000",
        "statusBar.background": "#0a0a0a",
        "statusBar.foreground": "#ffffff",
        "statusBar.border": "#ff0000",
        "titleBar.activeBackground": "#1a1a1a",
        "titlebar.inactiveBackground": "#0a0a0a",
        "badge.background": "#ff0000",
        "badge.foreground": "#ffffff",
        "statusBarItem.prominentBackground": "#ff0000",
        "tab.activeBorderTop": "#ff0000",
        "activityBar.foreground": "#ff0000",
        "activityBarBadge.background": "#ff0000",
        "activityBarBadge.foreground": "#ffffff",
        "list.activeSelectionBackground": "#ff000033",
        "list.activeSelectionForeground": "#ffffff",
        "list.inactiveSelectionBackground": "ff000020",
        "tree.indentGuidesStroke": "#ff0000",
        "scrollbarSlider.hoverBackground": "#ff0000",
        "dropdown.background": "#1a1a1a",
        "dropdown.border": "#ff0000"
    },
    "tokenColors": [
        {
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#666666",
                "fontStyle": "italic"
            }
        },
        {
            "scope": ["keyword"],
            "settings": {
                "foreground": "#ff0000",
                "fontStyle": "bold"
            }
        },
        {
            "scope": ["string"],
            "settings": {
                "foreground": "#ffd700"
            }
        },
        {
            "scope": ["variable", "identifier"],
            "settings": {
                "foreground": "#d0d0d0"
            }
        },
        {
            "scope": ["function", "entity.name.function"],
            "settings": {
                "foreground": "#ffffff"
            }
        },
        {
            "scope": ["invalid", "invalid.illegal"],
            "settings": {
                "foreground": "#ff3333",
                "fontStyle": "underline"
            }
        },
        {
            "scope": ["entity.name.type", "support.type"],
            "settings": {
                "foreground": "#ff0000"
            }
        },
        {
            "scope": ["constant.numeric", "constant.language"],
            "settings": {
                "foreground": "#ff8080"
            }
        }
    ]
}